{
    "name": "cmmv",
    "description": "Minimalistic Node.js Server Framework",
    "keywords": [
        "cmmv",
        "framework",
        "http",
        "contract"
    ],
    "scripts": {
        "prebuild": "rimraf dist && pnpm run clean",
        "postbuild": "pnpm run packages:move",
        "build": "rm -rf ./packages/**/*.tsbuildinfo && NODE_ENV=build tsc -b -v packages",
        "build:dev": "tsc -b -v packages --watch",
        "prebuild:prod": "pnpm run clean",
        "build:prod": "rm -rf ./packages/**/*.tsbuildinfo && tsc -b -v packages",
        "build:client": "vite build --ssrManifest --outDir dist/client",
        "prod:packages": "tsc -b -v packages",
        "build:test": "rm -rf ./packages/**/*.tsbuildinfo && tsc -b -v packages",
        "dev": "./tools/cleanupPackages.sh && pnpm run clean && NODE_ENV=dev nodemon --quiet",
        "start": "NODE_ENV=production node -r @swc-node/register ./src/main.ts",
        "start:prod": "NODE_ENV=production node -r @swc-node/register ./src/main.ts",
        "compile": "./tools/cleanupPackages.sh && pnpm run clean && NODE_ENV=dev node -r @swc-node/register ./src/compile.ts",
        "generate:keys": "./tools/cleanupPackages.sh && pnpm run clean && node -r @swc-node/register ./src/generateKeys.ts",
        "clean:tsfiles": "find ./src -type f \\( -name '*.d.ts' -o -name '*.js' \\) -exec rm -f {} +",
        "clean:packages": "find ./packages -name 'node_modules' -prune -o -type f \\( -name '*.d.ts' -o -name '*.js' \\) -exec rm -f {} +",
        "clean": "pnpm run clean:tsfiles && pnpm run clean:packages > /dev/null 2>&1",
        "format": "prettier \"**/*.ts\" \"packages/**/*.json\" --ignore-path ./.prettierignore --write && git status",
        "test": "NODE_ENV=test vitest",
        "test:coverage": "NODE_ENV=test vitest --coverage",
        "test:ui": "NODE_ENV=test vitest --ui",
        "test:watch": "NODE_ENV=test vitest --watch",
        "test:run": "NODE_ENV=test vitest run",
        "lint": "concurrently 'pnpm run lint:packages' 'pnpm run lint:spec'",
        "lint:fix": "concurrently 'pnpm run lint:packages -- --fix' 'pnpm run lint:spec -- --fix'",
        "lint:packages": "eslint 'packages/**/**.ts' --no-ignore",
        "lint:spec": "eslint 'packages/**/**.spec.ts' --no-ignore",
        "prerelease": "./tools/preRelease.sh && pnpm install && gulp copy-misc",
        "packages:move": "gulp move",
        "changelog": "lerna-changelog",
        "release:lerna": "./node_modules/.bin/lerna publish --force-publish --exact -m \"chore: chore(@cmmv) publish %s release\"",
        "release": "pnpm run build:prod && pnpm run packages:move && pnpm run release:lerna && pnpm run clean",
        "prepare": "husky",
        "typeorm": "node -r @swc-node/register ./node_modules/typeorm/cli.js",
        "migration:run": "pnpm typeorm migration:run -d src/migration.ts",
        "schema:sync": "pnpm typeorm schema:sync -d src/migration.ts",
        "migration:show": "pnpm typeorm migration:show -d src/migration.ts",
        "migration:generate": "pnpm typeorm migration:generate -d src/migration.ts",
        "migration:create": "pnpm typeorm migration:create -d src/migration.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cmmvio/cmmv.git"
    },
    "author": "Andre Ferreira",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/cmmvio/cmmv/issues"
    },
    "homepage": "https://cmmv.io",
    "engines": {
        "node": ">= 20.0.0"
    },
    "changelog": {
        "labels": {
            "type: feature :tada:": "Features",
            "type: bug :sob:": "Bug fixes",
            "type: enhancement :wolf:": "Enhancements",
            "type: docs :page_facing_up:": "Docs",
            "type: code style": "Code style tweaks",
            "dependencies": "Dependencies"
        }
    },
    "lint-staged": {
        "**/*.ts": [
            "prettier --ignore-path ./.prettierignore --write"
        ],
        "packages/**/*.json": [
            "prettier --ignore-path ./.prettierignore --write"
        ]
    },
